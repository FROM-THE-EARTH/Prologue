{
  "processing": {
    "multi_thread": true,
    "multi_thread_count": 16
  },

  "simulation": {
    "dt": 0.001,

    "scatter": {
      "wind_speed_min": 1.0,
      "wind_speed_max": 7.0,
      "wind_dir_interval": 2.5
    }
  },

  "result": {
    "precision": 8,
    "step_save_interval": 10
  },

  "wind_model": {
    "power_constant": 6.0,
    "power_low_base_alt": 2.0,
    "type": "only_powerlow",
    "realdata_filename": "sample.csv"
  },

  "atmosphere": {
    "base_pressure_pascal": 101325,
    "base_temperature_celsius": 15
  }
}
